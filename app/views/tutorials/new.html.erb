<h1>New Tutorial</h1>
<div class="form-group">
  <%= form_for @tutorial do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title, class: "form-control" %>
  <% end %>
</div>
<div class="form-group">
  <%= form_for @tutorial do |f| %>
    <%= f.label :content %>
    <%= f.text_area :content, class: "form-control" %>
  <% end %>
</div>
<div class="form-group">
  <%= form_for @tutorial do |f| %>
    <%= f.submit :submit, class: "btn btn-primary btn-lg" %>
  <% end %>
</div>
